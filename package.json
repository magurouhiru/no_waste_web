{
  "name": "no-waste-web",
  "version": "0.0.0",
  "scripts": {
    "ng": "no_waste_web/node_modules/.bin/ng",
    "start": "npx ng serve",
    "build": "npx ng build",
    "watch": "no_waste_web/node_modules/.bin/ng build --watch --configuration development",
    "test": "npx jest",
    "serve:ssr:no_waste_web": "node dist/no_waste_web/server/server.mjs",
    "lint": "ng lint",
    "lint --fix": "ng lint --fix",
    "prettier --fix": "npx prettier -w ./",
    "tool analysisWiki": "run-s tool_tsc tool_analysisWiki_node",
    "tool_tsc": "npx tsc -p ./tools/tsconfig.json",
    "tool_analysisWiki_node": "node ./tools/src/analysisWiki.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.0",
    "@angular/common": "^18.2.0",
    "@angular/compiler": "^18.2.0",
    "@angular/core": "^18.2.0",
    "@angular/forms": "^18.2.0",
    "@angular/platform-browser": "^18.2.0",
    "@angular/platform-browser-dynamic": "^18.2.0",
    "@angular/platform-server": "^18.2.0",
    "@angular/router": "^18.2.0",
    "@angular/ssr": "^18.2.1",
    "express": "^4.18.2",
    "primeng": "^17.18.9",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.1",
    "@angular/cli": "^18.2.1",
    "@angular/compiler-cli": "^18.2.0",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~5.1.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^18.18.0",
    "@typescript-eslint/eslint-plugin": "^8.4.0",
    "angular-eslint": "18.3.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^9.1.0",
    "jasmine-core": "~5.2.0",
    "jest": "^29.7.0",
    "jest-preset-angular": "^14.2.2",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "node-html-parser": "^6.1.13",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.45",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.10",
    "typescript": "~5.5.2",
    "typescript-eslint": "8.1.0"
  }
}
